{
  "name": "redux-libs-demo-app",
  "version": "0.1.0",
  "description": "A demo app to showcase, test, compare some Redux related libraries.",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "format": "prettier --loglevel warn --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --ext js,ts,tsx src",
    "lint-fix": "eslint --fix --ext js,ts,tsx src",
    "start": "react-scripts start",
    "start-https": "HTTPS=true SSL_CRT_FILE=dev.crt SSL_KEY_FILE=dev.key react-scripts start",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "type": "npm run type-check",
    "type-check": "tsc"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@reduxjs/toolkit": "1.4.0",
    "@vmw/slices-for-redux": "0.1.0",
    "@vmw/queue-for-redux-saga": "0.1.0",
    "connected-react-router": "6.8.0",
    "date-fns": "2.16.1",
    "history": "4.10.1",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "wretch": "1.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.7",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/redux-saga-tester": "1.0.2",
    "@vmw/eslint-config-vmware-react": "0.1.0",
    "eslint": "6.8.0",
    "react-scripts": "3.4.3",
    "redux-saga-tester": "1.0.874",
    "msw": "0.21.2",
    "prettier": "2.1.2",
    "query-string": "6.13.5",
    "typescript": "3.9.3"
  },
  "eslintConfig": {
    "extends": "@vmw/vmware-react",
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "node_modules",
            "src"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.tsx",
      "!src/common/test-utils/*",
      "!src/pages/things/view/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
